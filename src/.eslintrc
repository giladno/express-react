{
    "root": true,
    "globals": {
        "__DEV__": true,
        "_": true,
        "React": true
    },
    "rules": {
        "no-console": 0,
        "react/prop-types": 0,
        "react/display-name": 0,
        "react/jsx-no-bind": "warn",
        "react/no-render-return-value": 0
    },
    "env": {
        "amd": true,
        "es6": true,
        "commonjs": true,
        "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react"],
    "parser": "babel-eslint",
    "settings": {"react": {"version": "detect"}}
}
